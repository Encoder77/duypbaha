<div id="slider" class="section main_slider">
  <div class="container-fuild">
    <div class="row">
      <div id="rev_slider_4_1_wrapper" class="rev_slider_wrapper fullwidthbanner-container" data-alias="classicslider1" style="margin:0px auto;background-color:transparent;padding:0px;margin-top:0px;margin-bottom:0px;">
        <!-- START REVOLUTION SLIDER 5.0.7 auto mode -->
        <div id="rev_slider_4_1" class="rev_slider fullwidthabanner" style="display:none;" data-version="5.0.7">
          <ul>
            <% for(let i=0; i < banners.length; i++) { %>
            <li data-transition="zoomin" data-slotamount="7"  data-easein="Power4.easeInOut" data-easeout="Power4.easeInOut" data-masterspeed="2000"  data-thumb="/uploads/banners/<%- banners[i].pic_name %>"  data-rotate="0"  data-saveperformance="off"  data-title="<%- banners[i].head_text %>" data-description="">
              <video class="img-fluid"  autoplay loop muted>
                <source src="/uploads/banners/<%- banners[i].img_name %>" type="video/mp4" />
              </video>
            </li>
            <% } %>
          </ul>
          <div class="tp-static-layers"></div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="section padding_layout_1 blog_list">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12 pull-right">
        <div class="full">
          <% if(posts.length > 0) {%>
          <% for(let i=0; i < limit; i++) { %>
          <div class="blog_section">
          <section class="articles">
            <article>
              <div class="article-wrapper">
                <figure>
                  <img src="/uploads/posts/<%- posts[i].post_img %>" alt="" />
                </figure>
                <div class="article-body">
                  <h2><%- posts[i].title %></h2>
                  <div class="post_info">
                    <ul>
                      <li><i class="fa fa-comment" aria-hidden="true"></i> <%- posts[i].comment_count %></li>
                      <li><i class="fa fa-calendar" aria-hidden="true"></i> <%- posts[i].createdAt %></li>
                    </ul>
                  </div>
                  <p>Bahasy: <%- posts[i].excerpt %> TMT</p>
                  <a href="/<%- lang %>/blog/<%- posts[i].id %>" data-key="podrobnee" class="read-more">
                     <span class="sr-only">about this is some title</span>
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon" viewBox="0 0 20 20" fill="currentColor">
                      <path fill-rule="evenodd" d="M12.293 5.293a1 1 0 011.414 0l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-2.293-2.293a1 1 0 010-1.414z" clip-rule="evenodd" />
                    </svg>
                  </a>
                </div>
              </div>
            </article>
          </section>
        </div>
          <% } %>
          <% } %>
          <% if(pagecounter) { %>
          <div class="center">
            <ul class="pagination style_1 mb-5">
              <% for(let p=1;p <= pagecounter; p++) {%>
              <% if(pageNum==p) {%><li class="active"><%}else{%><li><%}%> <a href="/<%- lang %>/blog?page=<%- p %>&category=<%- url %>"><%- p %></a></li>
              <% } %>
            </ul>
          </div>
          <% } %>
        </div>
      </div>
  </div>
</div>
